<!-- calendar code from https://fullcalendar.io/docs/initialize-globals-->

<div class="container">
    <div class="row">
        <div class="col-sm-6">
<span id='calendar'>
    <script src='https://cdn.jsdelivr.net/npm/fullcalendar@6.1.8/index.global.min.js'></script>
        <script>

        document.addEventListener('DOMContentLoaded', function() {
            var calendarEl = document.getElementById('calendar');
            var calendar = new FullCalendar.Calendar(calendarEl, {
            initialView: 'dayGridMonth'
            });
            calendar.render();
            });

        </script>

</span>
        </div>
<div class="col-sm-6">
    <img src="/assets/homePage/test_groot_image.jpeg" alt="Mock event" class="groot">
</div>
